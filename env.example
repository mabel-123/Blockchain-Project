# ===========================================
# ENVIRONMENT VARIABLES TEMPLATE
# ===========================================
# Copy this file to .env and fill in your actual values
# Never commit your .env file to version control

# Alchemy API Key (Get from: https://www.alchemy.com/)
ALCHEMY_API_KEY=your_alchemy_api_key_here

# Private Key (Get from MetaMask: Account Details > Export Private Key)
# Should be 64 characters long, WITHOUT 0x prefix
SEPOLIA_PRIVATE_KEY=your_private_key_here

# Etherscan API Key (Get from: https://etherscan.io/apis)
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# Optional: Gas Reporter Settings
REPORT_GAS=true
COINMARKETCAP_API_KEY=your_coinmarketcap_api_key_here

# Optional: Network Configuration
NETWORK=sepolia

# Optional: Contract Verification
VERIFY_CONTRACTS=true 